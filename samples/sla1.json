{
  "sla": "1.0.0",
  "context": {
    "id": "petstore-sample",
    "type": "plans",
    "api": {
      "$ref": "./petstore-service.yml"
    },
    "provider": "ISAGroup"
  },
  "metrics": {
    "requests": {
      "type": "integer",
      "format": "int64",
      "description": "Number of requests"
    },
    "execution": {
      "type": "integer",
      "format": "int64",
      "description": "Execution time (ms)"
    }
  },
  "plans": {
    "base": {
      "availability": "R/00:00:00Z/23:00:00Z"
    },
    "free": {
      "rates": {
        "/pets/{id}": {
          "get": {
            "requests": [
              {
                "max": 1,
                "period": "second"
              },
              {
                "max": 45,
                "period": "minute"
              },
              {
                "max": 256,
                "period": "hour"
              }
            ],
            "execution": [
              {
                "max": 450,
                "period": "day"
              }
            ]
          }
        },
        "/pets": {
          "delete": {
            "requests": [
              {
                "max": 5,
                "period": "minute"
              }
            ]
          }
        }
      },
      "quotas": {
        "/pets": {
          "post": {
            "requests": [
              {
                "max": 10,
                "period": "minute"
              }
            ]
          }
        }
      }
    },
    "pro": {
      "pricing": {
        "cost": 5,
        "currency": "EUR",
        "billing": "monthly"
      },
      "rates": {
        " /pets/{id}": {
          "get": {
            "requests": [
              {
                "max": 1,
                "period": "second"
              }
            ]
          }
        }
      }
    }
  }
}
